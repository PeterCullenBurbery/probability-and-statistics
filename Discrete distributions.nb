 FormBox[
  RowBox[{
   RowBox[{"P", "(", 
    RowBox[{"A", "|", "B"}], ")"}], "=", 
   RowBox[{
    FractionBox[
     RowBox[{"P", "(", 
      RowBox[{"A", "\[Intersection]", "B"}], ")"}], 
     RowBox[{"P", "(", "B", ")"}]], "=", 
    RowBox[{
     FractionBox[
      RowBox[{"P", "(", 
       RowBox[{
        RowBox[{"263", "<=", "RedFish", "<=", "270"}], "\[And]", 
        RowBox[{"264", "<=", "RedFish", "<=", "268"}]}], ")"}], 
      RowBox[{"P", "(", 
       RowBox[{"263", "<=", "RedFish", "<=", "270"}], ")"}]], "=", 
     RowBox[{
      FractionBox[
       RowBox[{"P", "(", 
        RowBox[{"264", "<=", "RedFish", "<=", "268"}], ")"}], 
       RowBox[{"P", "(", 
        RowBox[{"263", "<=", "RedFish", "<=", "270"}], ")"}]], 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"because", " ", "263"}], "<=", "RedFish", "<=", 
        RowBox[{
        "270", " ", "is", " ", "already", " ", "true", " ", "if", " ", 
         "264"}], "<=", "RedFish", "<=", 
        RowBox[{"268", " ", "is", " ", "true"}]}], "*)"}], "=", 
      RowBox[{
       UnderoverscriptBox["\[Sum]", 
        RowBox[{"i", "=", "264"}], "268"], 
       RowBox[{"P", "(", 
        RowBox[{
         RowBox[{"RedFish", "==", "i"}], ",", 
         RowBox[{"i", "\[Distributed]", "\[ScriptCapitalD]"}]}], 
