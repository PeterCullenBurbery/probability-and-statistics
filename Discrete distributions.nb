   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"m1", "=", "10"}], ",", 
       RowBox[{"m2", "=", "15"}], ",", 
       RowBox[{"n", "=", "12"}], ",", 
       RowBox[{"w1", "=", "1"}], ",", 
       RowBox[{"w2", "=", "2"}]}], "}"}], ",", 
     RowBox[{"FisherHypergeometricDistribution", "[", 
      RowBox[{"n", ",", "m1", ",", 
